{
  "name": "cloudblox",
  "version": "1.4.1",
  "description": "A Promised-Based API Wrapper for Roblox's Open Cloud and their other Web APIs",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"lib/**/*.ts\" \"dist/**/*.js\"",
    "lint": "eslint . --ext .ts --ext .js",
    "test": "ts-node test/test"
  },
  "keywords": [
    "roblox",
    "api-wrapper",
